{
  "name": "@nielse63/generate-tests",
  "version": "0.0.3",
  "description": "Automatically generate jest specs for uncovered source files",
  "keywords": [
    "jest",
    "generator",
    "node-scripts"
  ],
  "author": "Erik Nielsen <erik@312development.com>",
  "homepage": "https://github.com/nielse63/node-scripts/tree/main/packages/generate-tests#readme",
  "license": "ISC",
  "main": "dist/generate-tests.js",
  "module": "dist/generate-tests.js",
  "types": "dist/generate-tests.d.ts",
  "files": [
    "bin",
    "dist"
  ],
  "bin": {
    "generate-tests": "bin/generate-tests"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nielse63/node-scripts.git"
  },
  "scripts": {
    "test": "echo \"Error: run tests from root\" && exit 1",
    "build": "BABEL_ENV=build babel src --root-mode upward --out-dir dist --extensions .ts,.js --delete-dir-on-start --no-comments",
    "build:types": "tsc --project tsconfig.build.json"
  },
  "dependencies": {
    "@nielse63/helpers": "^0.0.3",
    "commander": "8.3.0",
    "fast-glob": "3.2.7",
    "fs-extra": "10.0.0",
    "lodash": "4.17.21"
  },
  "bugs": {
    "url": "https://github.com/nielse63/node-scripts/issues"
  },
  "devDependencies": {
    "@types/fs-extra": "9.0.13",
    "@types/lodash": "4.14.178"
  }
}
