{
  "name": "@nielse63/copy-env",
  "version": "0.0.1",
  "description": "Format and copy .env files to env.sample to keep up to date",
  "keywords": [
    "env",
    "envlint",
    "node-scripts"
  ],
  "author": "Erik Nielsen <erik@312development.com>",
  "homepage": "https://github.com/nielse63/node-scripts/tree/main/packages/copy-env#readme",
  "license": "ISC",
  "main": "dist/copy-env.js",
  "module": "dist/copy-env.js",
  "types": "dist/copy-env.d.ts",
  "files": [
    "bin",
    "dist"
  ],
  "bin": {
    "copy-env": "bin/copy-env"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nielse63/node-scripts.git"
  },
  "scripts": {
    "test": "echo \"Error: run tests from root\" && exit 1",
    "build": "BABEL_ENV=build babel src --root-mode upward --out-dir dist --extensions .ts,.js --delete-dir-on-start --no-comments",
    "build:types": "tsc --project tsconfig.build.json"
  },
  "dependencies": {
    "commander": "^8.2.0",
    "fs-extra": "^10.0.0",
    "signale": "^1.4.0"
  },
  "bugs": {
    "url": "https://github.com/nielse63/node-scripts/issues"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.13",
    "@types/signale": "^1.4.2"
  }
}
