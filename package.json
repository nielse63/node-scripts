{
  "name": "node-scripts",
  "version": "1.1.0",
  "description": "Collection of useful node scripts and utilities",
  "scripts": {
    "test": "jest",
    "lint": "prettier --write . && eslint --fix .",
    "release": ".bin/release",
    "prepare": "husky install",
    "build": "npm run build --if-present --workspaces",
    "postbuild": ".bin/post-build.js",
    "generate": "plop package --plopfile config/plopfile.js",
    "clean": ".bin/clean.js",
    "update": ".bin/update-deps.js",
    "docs": ".bin/docs.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nielse63/node-scripts.git"
  },
  "keywords": [
    "node",
    "template",
    "github-template"
  ],
  "author": {
    "name": "Erik Nielsen",
    "email": "erik@312development.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nielse63/node-scripts/issues"
  },
  "homepage": "https://github.com/nielse63/node-scripts#readme",
  "engines": {
    "node": "^16.19.1",
    "npm": "^8.19.3"
  },
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-typescript": "^7.21.5",
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "@nielse63/eslint-config": "^1.1.0",
    "@nielse63/exec": "^1.1.0",
    "@nielse63/prettier-config": "^1.1.0",
    "@nielse63/tsconfig": "^1.1.0",
    "@nielse63/tsconfig-eslint": "^1.1.0",
    "@types/fs-extra": "^11.0.1",
    "@types/jest": "^29.5.1",
    "@types/lodash": "^4.14.194",
    "@types/node": "^18.16.14",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "babel-jest": "^29.5.0",
    "eslint": "^8.41.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-prettier": "^4.2.1",
    "fast-glob": "^3.2.12",
    "generate-changelog": "^1.8.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-extended": "^3.2.4",
    "lint-staged": "^13.2.2",
    "lodash": "^4.17.21",
    "plop": "^3.1.2",
    "prettier": "^2.8.8",
    "typescript": "^5.0.4"
  },
  "prettier": "@nielse63/prettier-config",
  "lint-staged": {
    "*.{js,ts,json,md,yml,html}": [
      "prettier --write"
    ]
  },
  "workspaces": [
    "packages/*"
  ]
}
