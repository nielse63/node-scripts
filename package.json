{
  "name": "node-scripts",
  "version": "1.3.0",
  "description": "Collection of useful node scripts and utilities",
  "scripts": {
    "test": "jest",
    "test:coverage": "npm run test -- --coverage",
    "lint": "eslint --fix . && prettier --write .",
    "release": ".bin/release",
    "prepare": "husky || true",
    "build": "npm run build --if-present --workspaces",
    "postbuild": ".bin/post-build.js",
    "generate": "plop package --plopfile config/plopfile.js",
    "clean": ".bin/clean.js",
    "update": ".bin/update-deps.js",
    "docs": ".bin/docs.js",
    "verify": ".bin/verify"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nielse63/node-scripts.git"
  },
  "keywords": [
    "node",
    "template",
    "github-template"
  ],
  "author": {
    "name": "Erik Nielsen",
    "email": "erik@312development.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nielse63/node-scripts/issues"
  },
  "homepage": "https://github.com/nielse63/node-scripts#readme",
  "engines": {
    "node": ">=22.22.0"
  },
  "devDependencies": {
    "@babel/core": "^7.29.0",
    "@babel/preset-env": "^7.29.0",
    "@babel/preset-typescript": "^7.28.5",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@nielse63/eslint-config": "^1.3.0",
    "@nielse63/exec": "^1.3.0",
    "@nielse63/prettier-config": "^1.3.0",
    "@nielse63/tsconfig": "^1.3.0",
    "@nielse63/tsconfig-eslint": "^1.3.0",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.23",
    "@types/node": "^22.19.11",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "babel-jest": "^29.7.0",
    "eslint": "^8.57.1",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest": "^28.14.0",
    "eslint-plugin-jest-extended": "^2.4.0",
    "eslint-plugin-prettier": "^5.5.5",
    "fast-glob": "^3.3.3",
    "generate-changelog": "^1.8.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-extended": "^4.0.2",
    "lint-staged": "^15.5.2",
    "lodash": "^4.17.23",
    "plop": "^4.0.5",
    "prettier": "^3.8.1",
    "typescript": "5.3.3"
  },
  "prettier": "@nielse63/prettier-config",
  "lint-staged": {
    "*.{js,ts,json,md,yml,html}": [
      "prettier --write"
    ]
  },
  "workspaces": [
    "packages/*"
  ]
}
